{
	"name": "angular-upgrade-prototype",
	"version": "0.1.0",
	"description": "Sample AngularJS project for prototyping Angular upgrade strategies",
	"main": "index.js",
	"scripts": {
		"build": "ng build",
		"format": "prettier --ignore-path \"node_modules/**\" --write \"**/*.{ts,js,json,scss,css}\"",
		"start": "ng serve",
		"test": "npm run test:legacy && npm run test:modern",
		"test:legacy": "cross-env NODE_ENV=karma karma start ./karma-legacy.conf.js --single-run",
		"test:legacy:watch": "cross-env NODE_ENV=karma karma start ./karma-legacy.conf.js",
		"test:modern": "ng test",
		"ng": "ng",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ericgibby/angular-upgrade-prototype.git"
	},
	"keywords": [
		"Angular",
		"AngularJS",
		"ngUpgrade"
	],
	"author": "Eric Gibby",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ericgibby/angular-upgrade-prototype/issues"
	},
	"homepage": "https://github.com/ericgibby/angular-upgrade-prototype#readme",
