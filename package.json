{
	"name": "angular-upgrade-prototype",
	"version": "0.1.0",
	"description": "Sample AngularJS project for prototyping Angular upgrade strategies",
	"main": "index.js",
	"scripts": {
		"build": "webpack -p",
		"format": "prettier --ignore-path \"node_modules/**\" --write \"**/*.{ts,js,json,scss,css}\"",
		"start": "webpack-dev-server",
		"test": "cross-env NODE_ENV=karma karma start ./karma.conf.js --single-run",
		"test:watch": "cross-env NODE_ENV=karma karma start ./karma.conf.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ericgibby/angular-upgrade-prototype.git"
	},
	"keywords": [
		"Angular",
		"AngularJS",
		"ngUpgrade"
	],
	"author": "Eric Gibby",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ericgibby/angular-upgrade-prototype/issues"
	},
	"homepage": "https://github.com/ericgibby/angular-upgrade-prototype#readme",
	"dependencies": {
		"@babel/runtime": "^7.6.0",
		"@uirouter/angularjs": "^1.0.22",
		"angular": "^1.7.9",
		"foundation-sites": "^6.5.3"
	},
	"devDependencies": {
		"@babel/core": "^7.6.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/plugin-transform-runtime": "^7.6.0",
		"@babel/preset-env": "^7.6.0",
		"angular-mocks": "^1.7.8",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"cross-env": "^6.0.0",
		"css-loader": "^3.2.0",
		"exports-loader": "^0.7.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^3.0.5",
		"karma": "^6.3.14",
		"karma-babel-preprocessor": "^8.0.1",
		"karma-chrome-launcher": "^3.1.0",
		"karma-jasmine": "^2.0.1",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-spec-reporter": "0.0.32",
		"karma-webpack": "^4.0.2",
		"ngtemplate-loader": "^2.0.1",
		"node-sass": "^4.12.0",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1",
		"sass-loader": "^8.0.0",
		"source-map-loader": "^0.2.4",
		"style-loader": "^1.0.0",
		"webpack": "^4.40.2",
		"webpack-cli": "^3.3.9",
		"webpack-dev-server": "^3.8.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
